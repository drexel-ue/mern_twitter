{
  "name": "twitter",
  "version": "1.0.0",
  "description": "MERN stack replication of Twitter",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "server": "nodemon app.js",
    "server:debug": "nodemon --inspect app.js",
    "frontend-install": "npm install --prefix frontend",
    "frontend": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run frontend\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix frontend && npm run build --prefix frontend"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/drexel-ue/mern_twitter.git"
  },
  "author": "IKesh Pack <packappsforyou@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/drexel-ue/mern_twitter/issues"
  },
  "homepage": "https://github.com/drexel-ue/mern_twitter#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^4.1.1",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.6.9",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "nodemon": "^1.19.1"
  }
}
